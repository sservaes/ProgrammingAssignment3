complete <- function(directory, id) {
# Initiate list
nobs <- c()
# Loop through files
for(i in id){
readNew <- read.csv(paste(directory, "/", formatC(i, width = 3, flag = "0"),
".csv", sep = ""))
good1 <- complete.cases(readNew[,"sulfate"])
good2 <- complete.cases(readNew[,"nitrate"])
good <- good1 * good2
nobs <- c(nobs, length(good[good == TRUE]))
}
# Output
value = cbind(id, nobs)
result = as.data.frame(value)
return(result)
}
